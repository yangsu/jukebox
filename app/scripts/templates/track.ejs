<input id="progress" type="text" class="readonly dial" data-min="0" data-max="100">
<input id="seek" type="text" class="dial" data-min="0" data-max="100">

<div id="controls">
    <label for="filter">Filter</label>
    <input id="filter" type="range" min="0" max="1" step="0.01" value="1">
    <label for="volume">Volume</label>
    <input id="volume" type="range" min="0" max="1" step="0.01" value="1">
    <div id="musicControls">
        <button id="play" disabled>Play</button>
        <button id="pause" disabled>Pause</button>
        <button id="stop" disabled>Stop</button>
        <button id="next" disabled>Next</button>
        <button id="previous" disabled>Previous</button>
    </div>
</div>

<canvas id="fftcanvas" width="600" height="400"></canvas>
<canvas id="fftcanvas2" width="600" height="400"></canvas>